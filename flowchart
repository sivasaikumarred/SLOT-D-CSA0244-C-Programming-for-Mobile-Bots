#include<stdio.h>
int main()
{
	int count=1,p,n,r;
	float si;
	printf("Enter value of P: ");
	scanf("%d",&p);
	printf("Enter value of N: ");
	scanf("%d",&n);
	printf("Enter value of R: ");
	scanf("%d",&r);
	while(count<=3)
	{
		si=p*n*r/100;
		printf("%f\n",si);
	    count=count+1;
	}
	
	return 0;
}
